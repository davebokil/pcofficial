        <div class="row section-header narrow" data-aos="fade-up">
            <div class="col-full">
                <h3>Contact</h3>
                <!-- <h1 class="display-1">Feel free to contact us for any <br> project idea or collaboration</h1> -->
            </div>
        </div>
        <!-- end section-header -->
        <div class="row contact-main" data-aos="fade-up">
            <div class="col-full">
                <form method="post" action="send">
                    <div>
                        <label for="nameInput">Your name</label>
                        <input class="full-width contactformborder" type="text" placeholder="Enter name" id="nameInput" name="name">
                    </div>
                    <div>
                        <label for="sampleInput">Your email</label>
                        <input class="full-width contactformborder" type="email" placeholder="Enter email" id="sampleInput" name="email">
                    </div>
                    <div>
                        <label for="sampleRecipientInput">Reason for contacting</label>
                        <div class="ss-custom-select contactformborder">
                            <select class="full-width" id="sampleRecipientInput" name="inquiry">
                                <option value="General Question">General Question</option>
                                <option value="Business Inquiry">Business Inquiry</option>
                                <option value="Appearances">Appearances</option>
                                <option value="Mentoring">Mentoring</option>
                                
                            </select>
                        </div>
                    </div>
                    <label for="exampleMessage">Message</label>
                    <textarea class="full-width contactformborder" placeholder="Your message" id="exampleMessage" name="message"></textarea>
                    <input class="btn full-width" type="submit" value="Submit" style="color: #fff">
                </form>
                <h3 id="sent">{{msg}}</h3>

                
